cc_library_shared {
    name: "liblineage-sdk_platform_jni",
    srcs: [
        "src/org_lineageos_platform_internal_LineageAudioService.cpp",
        "src/onload.cpp",
    ],

    include_dirs: [
        "frameworks/base/core/jni",
        "frameworks/av/include",
        "hardware/libhardware/include",
